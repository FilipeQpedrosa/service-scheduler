{
  "version": 2,
  "framework": "nextjs",
  "buildCommand": "prisma generate && next build",
  "installCommand": "npm install",
  "env": {
    "NEXTAUTH_URL": "https://${VERCEL_URL}",
    "VERCEL_ENV": "production"
  },
  "regions": ["sfo1"],
  "functions": {
    "app/api/**/*": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
} 