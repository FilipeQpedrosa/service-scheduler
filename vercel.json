{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/staff/(.*)",
      "dest": "/staff/$1"
    },
    {
      "src": "/business/(.*)",
      "dest": "/business/$1"
    },
    {
      "src": "/dashboard/(.*)",
      "dest": "/dashboard/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
} 